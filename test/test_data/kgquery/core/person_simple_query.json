{
    "https://schema.hbp.eu/graphQuery/root_schema": {
        "@id": "https://nexus.humanbrainproject.org/v0/schemas/neuralactivity/core/person/v0.1.0"
    },
    "http://schema.org/identifier": "neuralactivity/core/person/v0.1.0/fgSimple",
    "fields": [
        {
            "filter": {
                "op": "equals",
                "parameter": "id"
            },
            "relative_path": "@id"
        },
        {
            "relative_path": "@type"
        },
        {
            "fieldname": "familyName",
            "filter": {
                "op": "contains",
                "parameter": "family_name"
            },
            "relative_path": "http://schema.org/familyName"
        },
        {
            "fieldname": "givenName",
            "filter": {
                "op": "contains",
                "parameter": "given_name"
            },
            "relative_path": "http://schema.org/givenName"
        },
        {
            "fieldname": "email",
            "filter": {
                "op": "contains",
                "parameter": "email"
            },
            "relative_path": "http://schema.org/email"
        },
        {
            "fieldname": "affiliation",
            "filter": {
                "op": "contains",
                "parameter": "affiliation"
            },
            "fields": [
                {
                    "relative_path": "@id"
                },
                {
                    "relative_path": "@type"
                }
            ],
            "relative_path": "http://schema.org/affiliation"
        }
    ],
    "@context": {
        "fieldname": {
            "@type": "@id",
            "@id": "fieldname"
        },
        "schema": "http://schema.org/",
        "@vocab": "https://schema.hbp.eu/graphQuery/",
        "nsg": "https://bbp-nexus.epfl.ch/vocabs/bbp/neurosciencegraph/core/v0.1.0/",
        "merge": {
            "@type": "@id",
            "@id": "merge"
        },
        "query": "https://schema.hbp.eu/myQuery/",
        "dcterms": "http://purl.org/dc/terms/",
        "relative_path": {
            "@type": "@id",
            "@id": "relative_path"
        }
    }
}